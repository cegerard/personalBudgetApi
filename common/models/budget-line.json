{
  "name": "budgetLine",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "budget": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "period": {
      "type": "string",
      "required": true,
      "default": "monthly"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "endDate": {
      "type": "date"
    },
    "totalExpense": {
      "type": "number",
      "default": 0
    }
  },
  "validations": [],
  "relations": {
    "expenses": {
      "type": "hasMany",
      "model": "expense",
      "foreignKey": "budgetline"
    }
  },
  "acls": [],
  "methods": {}
}
