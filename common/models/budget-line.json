{
  "name": "budgetLine",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "budget": {
      "type": "number",
      "required": true
    },
    "name": {
      "type": "string",
      "required": true
    },
    "period": {
      "type": "string",
      "required": true,
      "default": "monthly"
    },
    "startDate": {
      "type": "date",
      "required": true
    },
    "description": "string"
  },
  "validations": [],
  "relations": {
    "createdBy": {
      "type": "belongsTo",
      "model": "endUser",
      "foreignKey": "ownerId"
    },
    "expenses": {
      "type": "hasMany",
      "model": "expense",
      "foreignKey": "budgetLine"
    }
  },
  "acls": [
    {
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY",
      "accessType": "*"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "accessType": "READ"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "WRITE"
    },
    {
      "principalType": "ROLE",
      "principalId": "admin",
      "permission": "ALLOW",
      "property": "__get__expenses"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__expenses"
    },
    {
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__expenses"
    }
  ],
  "methods": {}
}
